{
    "patterns": [
        {
            "pattern": "(?:(?:\\+|1[-. ]?)?(?:\\d{1,3}[-. ]?)?)?\\(?\\d{3}\\)?[-. ]?(?:\\d{4}|\\d{1,2} \\d{4}|\\d{7})",
            "replacement": "111-111-1111"
        },
        {
            "pattern": "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
            "replacement": "Test_user@example.com"
        },
        {
            "pattern": "\\s*(?i)[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,}$|^(?i)[\\p{L}\\p{N}._%+-]+@[\\p{L}\\p{N}.-]+\\.[\\p{L}]{2,}\\s*$",
            "replacement": "Test_user@example.com"
        },
        {
            "pattern": "(?:\\+\\d{3}\\s\\d{3}|\\d{2}\\s\\d{5})",
            "replacement": "111-111-1111"
        }
    ]
}